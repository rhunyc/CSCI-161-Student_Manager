    private void saveFile(File file)
    {
        try
        {
            FileOutputStream fos = new FileOutputStream(file);
            ObjectOutputStream oos = new ObjectOutputStream(fos);
            oos.writeObject(studentList);
        }
        catch (Exception e)
        {
            JOptionPane.showMessageDialog(this, "Could not save file!");
        }
    }
    
    private void loadFile(File file)
    {
        try
        {
            FileInputStream fis = new FileInputStream(file);
            ObjectInputStream ois = new ObjectInputStream(fis);
            ArrayList<Student> studentList = (ArrayList<Student>)ois.readObject();
            
        }
        catch (Exception e)
        {
            JOptionPane.showMessageDialog(this, "Could not load file!");
            StudentFactory.generateArrayListStudents(20);
            
        }
    }
    
    private void populateAdminUserList(){
      if (!Administrator.accessStudent().isEmpty()){
        for (int i = 0; i < Administrator.accessStudent().size(); i++){
        model.add(i, Administrator.accessStudent().get(i).getID());
    }
        aUserList.setModel(model);
      }
    }
        

